                    <exclude name="verifySubTaskFromCreatedTicket" />
                    <exclude name="verifyDeleteBoardfromActivatedSprint" />
                    <exclude name="verifyReportsSection" />
                    <exclude name="verifySprintCompletedConfirmation" />
                    <exclude name="verifyWorkloadSettingsUpdate" />
                    <exclude name="verifySkipSpSuccessfullySprintComplete" />
