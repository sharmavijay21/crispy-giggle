                    <include name="verifyCreateNewProject"/>
                    <exclude name="verifyCreateTeamBoard"/>  
                    <exclude name="verifyAddCustomFieldAndDelete"/> 
                    <exclude name="verifyAddBoardAndDelete"/> 
                    <exclude name="verifyNotificationsSettings"/>
                    <exclude name="verifyNotificationandMarkAllasRead"/>
                    <exclude name="verifyOpenNotificationpage"/>
                </methods>
            </class>  

            <class name="com.PlanIT.WitsLab.tests.BacklogTestScript">
